<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffff8c00" android:endColor="#ffffffff" android:angle="270.0" />
            <corners android:topLeftRadius="8.0dip" android:topRightRadius="8.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffffc2b7" android:endColor="#ffffc2b7" android:angle="270.0" />
            <corners android:topLeftRadius="8.0dip" android:topRightRadius="8.0dip" />
        </shape>
    </item>
</selector>